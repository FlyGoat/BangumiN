{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "align": [
      true,
      "parameters"
    ],
    "import-name": false,
    "object-curly-spacing": false,
    "max-line-length": [
      true,
      140
    ],
    "no-console": true
  }
}